{
  "name": "aslnbxrz/filament-simple-translation",
  "description": "Filament 4 page to manage Simple Translation keys & translations inline.",
  "type": "library",
  "version": "2.0.0",
  "require": {
    "php": "^8.1",
    "filament/filament": "^4.0",
    "livewire/livewire": "^3.0",
    "aslnbxrz/simple-translation": "^1.0",
    "spatie/laravel-package-tools": "^1.92"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Aslnbxrz\\FilamentSimpleTranslation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "aslnbxrz",
      "email": "bexruz.aslonov1@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Aslnbxrz\\FilamentSimpleTranslation\\FilamentSimpleTranslationServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^10.0",
    "pestphp/pest": "^4.0",
    "pestphp/pest-plugin-laravel": "^4.0",
    "nunomaduro/collision": "^8.0",
    "mockery/mockery": "^1.6",
    "laravel/pint": "^1.16"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "pest",
    "lint": "pint",
    "test:coverage": "pest --coverage"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "/Users/aslnbxrz/Desktop/Composer Packages/SimpleTranslation",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "/Users/aslnbxrz/Desktop/Composer Packages/SimplePrefix",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "/Users/aslnbxrz/Desktop/Composer Packages/SimpleException",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "/Users/aslnbxrz/Desktop/Composer Packages/FilamentTranslation",
      "options": {
        "symlink": true
      }
    }
  ]
}
